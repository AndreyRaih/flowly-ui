{
  "name": "flowly-ui",
  "version": "0.0.1",
  "description": "Template for Flowly UI development",
  "main": "index.js",
  "keywords": [
    "flowly"
  ],
  "author": "andreyraih.dev@gmail.com",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "format": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "hooks:pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix"
  },
  "dependencies": {
    "@preact/signals": "^1.1.5",
    "chart.js": "^4.3.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@iameax/eslint-plugin": "^0.0.20",
    "@preact/preset-vite": "^2.5.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animated": "^1.0.0",
    "tailwindcss-safe-area": "^0.2.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
